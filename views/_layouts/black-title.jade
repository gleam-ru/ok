extends ./default.jade

block append head
  style.
    .sticky-nav.nav-border-bottom {border-bottom: 1px solid rgba(0,0,0,0.2);}
    .cnt {min-height: calc(100vh - 279px);}
    .cnt > section {padding: 50px 0px;}
    .breadcrumb * {color: #f7f7f7 !important;}


block bc
  section.content-top-margin.page-title.page-title-small.bg-dark-gray
    .container
      .row
        .col-md-8.col-sm-12.wow.fadeInUp(data-wow-duration='300ms')
          h1.white-text.title
            block title
              | #{title || 'Title goes here'}
        include ./parts/breadcrumbs
